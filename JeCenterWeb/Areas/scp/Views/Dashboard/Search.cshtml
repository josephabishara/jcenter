@model IEnumerable<JeCenterWeb.Models.ApplicationUser>

@{
    ViewData["Title"] = "Students";
}

<table class="table table-striped table-sm text-center">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FullName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Mobile)
            </th>

        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    <a class="btn btn-block" asp-controller="Students" asp-action="Details" asp-route-id="@item.Id">
                        @item.FullName
                    </a>
                </td>
                <td>
                    <a class="btn btn-block" asp-controller="Students" asp-action="Details" asp-route-id="@item.Id">
                        @item.Mobile
                    </a>
                </td>

            </tr>
        }
    </tbody>
</table>




