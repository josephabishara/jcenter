@model JeCenterWeb.Models.PhysicalYear

@{
    ViewData["Title"] = "Create";
}


<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="PhysicalyearName" class="control-label"></label>
                <input asp-for="PhysicalyearName" class="form-control" />
                <span asp-validation-for="PhysicalyearName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="FromDate" class="control-label"></label>
                <input asp-for="FromDate" class="form-control" type="date" />
                <span asp-validation-for="FromDate" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ToDate" class="control-label"></label>
                <input asp-for="ToDate" class="form-control" type="date" />
                <span asp-validation-for="ToDate" class="text-danger"></span>
            </div>
           
            <div class="form-group">
                <input type="submit" value="حقظ" class="btn btn-info" /> |
                <a class="btn btn-dark" asp-action="Index"> رجوع </a>
            </div>
        </form>
    </div>
</div>

