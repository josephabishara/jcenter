@model IEnumerable<JeCenterWeb.Models.CGroupSchedule>

@{
    ViewData["Title"] = "Teachers";
}

<div class="row">
    <div class="col-3"><h3>@ViewData["today"]</h3>  </div>
    <div class="col-3"><a asp-action="TeachersSchedule" class="btn btn-block btn-info"><i class="fa fa-chalkboard-teacher"></i>  المدرسين</a></div>
    <div class="col-3"><a asp-action="RoomSchedule" class="btn btn-block btn-success"><i class="fa fa-braille"></i>  القاعة</a></div>
    <div class="col-3"><a asp-action="SessionStartSchedule" class="btn btn-block btn-primary"><i class="fa fa-clock"></i>  الوقت </a></div>
</div>
<table class="table table-striped table-sm text-center">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.CGroups.TeacherName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CGroups.CRooms.RoomName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CGroups.SessionStart)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CGroups.SessionEnd)
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @item.CGroups.TeacherName
                </td>
                <td>
                    @item.CGroups.CRooms.RoomName
                </td>
                <td>
                    @item.CGroups.SessionStart
                </td>
                <td>
                    @item.CGroups.SessionEnd
                </td>
            </tr>
        }
    </tbody>
</table>